{
  "expo": {
    "name": "SkillCart",
    "slug": "SkillCart",
    "icon": "./src/assets/splash.png",
    "version": "1.0.0",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "orientation": "portrait",
    "scheme": "SkillCart",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "entryPoint": "./index.ts",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rohit1991.SkillCart"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rohit1991.SkillCart"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "5f08fafb-ea1d-4fc8-9eae-b1fd8f7be8db"
      }
    },
    "owner": "rohit1991"
  }
}
