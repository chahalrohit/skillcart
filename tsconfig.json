{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@navigation/*": [
        "navigation/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@types/*": [
        "types/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@theme/*": [
        "theme/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "**/node_modules",
    "**/Pods"
  ],
  "extends": "expo/tsconfig.base"
}
